    <platform name="wp8">
        <splash height="1280" src="res/screens/wp8/SplashScreenImage.png" width="768" />
    </platform>
    <plugin name="cordova-plugin-compat" version="1.0.0" />
    <plugin name="cordova-plugin-geolocation" version="2.4.0" />
    <plugin name="cordova-plugin-network-information" version="1.3.0" />
    <plugin name="cordova-plugin-splashscreen" version="4.0.0" />
    <plugin name="com.cordova.plugins.cookiemaster" src="https://github.com/kristianhristov/cordova-cookie-master.git" version="1.0.0" />
    <vs:toolsetVersion>6.3.1</vs:toolsetVersion>
    <engine name="android" spec="^5.2.1" />
    <engine name="ios" spec="4.2.0" />
    <engine name="windows" spec="4.4.2" />
